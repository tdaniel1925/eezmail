{
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "nextjs",
  "regions": ["iad1"],
  "crons": [
    {
      "path": "/api/cron/process-scheduled-emails",
      "schedule": "* * * * *"
    },
    {
      "path": "/api/cron/renew-webhooks",
      "schedule": "0 0 * * *"
    },
    {
      "path": "/api/cron/process-embeddings",
      "schedule": "*/30 * * * *"
    },
    {
      "path": "/api/cron/process-timeline-events",
      "schedule": "*/30 * * * *"
    }
  ]
}
